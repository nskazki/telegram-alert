{
  "name": "telegram-alert",
  "version": "1.0.0",
  "description": "$ sleep 5; talert",
  "main": "app-build/index.js",
  "dependencies": {
    "bellman": "^1.0.2",
    "bluebird": "^2.9.34",
    "body-parser": "^1.13.3",
    "commander": "^2.8.1",
    "express": "^4.13.3",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "natan": "^1.0.2",
    "node-telegram-bot-api": "^0.12.1",
    "proxyquire": "^1.7.2",
    "simple-debugger": "^1.0.2"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "eslint": "^1.3.1",
    "eslint-plugin-mocha": "^0.5.1",
    "mocha": "^2.3.2"
  },
  "scripts": {
    "mocha": "mocha test/test*.es6 && rm -f storages/test-*-token2chats.json",
    "mocha-debug": "DEBUG=debug* LOGGER=true npm run mocha",
    "check-configs": "natan-test-configs -d configs",
    "eslint": "eslint $(find app test -type f -name '*.es6')",
    "test": "npm run eslint && npm run check-configs && npm run mocha",
    "start": "babel-node -- app/ -c configs/config.json",
    "start-debug": "DEBUG=debug* babel-node -- app/ -c configs/config.json -p true",
    "build": "babel --retain-lines app --out-dir app-build",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nskazki/telegram-alert.git"
  },
  "keywords": [
    "telegram",
    "alert",
    "talert"
  ],
  "author": "nskazki@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nskazki/telegram-alert/issues"
  },
  "homepage": "https://github.com/nskazki/telegram-alert#readme"
}
